{
  "navigationFallback": {
    "rewrite": "/agent-feedback-session.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "routes": [
    {
      "route": "/",
      "rewrite": "/agent-feedback-session.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/agent-feedback-session.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https://script.google.com https://formspree.io; img-src 'self' data: https:; font-src 'self' data:;"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".html": "text/html"
  }
}
